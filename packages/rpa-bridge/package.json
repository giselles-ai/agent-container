{
  "name": "@giselles/rpa-bridge",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./react": {
      "types": "./src/react/index.ts",
      "default": "./src/react/index.ts"
    },
    "./next": {
      "types": "./src/next/index.ts",
      "default": "./src/next/index.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm --out-dir dist --clean",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "format": "pnpm exec biome check --write ."
  },
  "dependencies": {
    "@giselles/rpa-sdk": "workspace:*",
    "@vercel/sandbox": "^1.0.0",
    "ioredis": "^5.9.2",
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0",
    "zod": "4.3.6"
  },
  "peerDependencies": {
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0"
  },
  "devDependencies": {
    "@types/react": "19.2.10",
    "@types/react-dom": "19.2.3",
    "@types/node": "25.0.10",
    "tsup": "8.5.1",
    "typescript": "5.9.3"
  }
}
