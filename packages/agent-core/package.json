{
	"name": "@giselles-ai/sandbox-agent-core",
	"version": "0.1.4",
	"type": "module",
	"sideEffects": false,
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/giselles-ai/agent-container.git",
		"directory": "packages/agent-core"
	},
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		}
	},
	"scripts": {
		"build": "pnpm clean && tsup --config tsup.ts",
		"clean": "rm -rf dist *.tsbuildinfo",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"format": "pnpm exec biome check --write ."
	},
	"dependencies": {
		"@giselles-ai/browser-tool": "workspace:*",
		"@vercel/sandbox": "^1.0.0",
		"ioredis": "^5.9.2",
		"zod": "4.3.6"
	},
	"devDependencies": {
		"@google/gemini-cli-core": "^0.28.2",
		"@types/node": "25.0.10",
		"tsup": "8.5.1",
		"typescript": "5.9.3"
	}
}
