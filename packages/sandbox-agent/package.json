{
	"name": "@giselles-ai/sandbox-agent",
	"version": "0.1.10",
	"type": "module",
	"sideEffects": false,
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/giselles-ai/agent-container.git",
		"directory": "packages/sandbox-agent"
	},
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		}
	},
	"scripts": {
		"build": "pnpm clean && tsup --config tsup.ts",
		"clean": "rm -rf dist *.tsbuildinfo",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"test": "pnpm exec vitest run",
		"format": "pnpm exec biome check --write ."
	},
	"dependencies": {
		"@vercel/sandbox": "1.6.0",
		"@iarna/toml": "3.0.0",
		"zod": "4.3.6"
	},
	"devDependencies": {
		"@google/gemini-cli-core": "0.29.5",
		"@types/node": "25.3.0",
		"@types/react": "19.2.14",
		"@types/react-dom": "19.2.3",
		"tsup": "8.5.1",
		"typescript": "5.9.3"
	}
}
