{
	"name": "@giselles-ai/browser-tool",
	"version": "0.1.7",
	"type": "module",
	"sideEffects": false,
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/giselles-ai/agent-container.git",
		"directory": "packages/browser-tool"
	},
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./react": {
			"types": "./dist/react/index.d.ts",
			"import": "./dist/react/index.js",
			"default": "./dist/react/index.js"
		},
		"./dom": {
			"types": "./dist/dom/index.d.ts",
			"import": "./dist/dom/index.js",
			"default": "./dist/dom/index.js"
		},
		"./mcp-server": {
			"types": "./dist/mcp-server/index.d.ts",
			"import": "./dist/mcp-server/index.js",
			"default": "./dist/mcp-server/index.js"
		},
		"./relay": {
			"types": "./dist/relay/index.d.ts",
			"import": "./dist/relay/index.js",
			"default": "./dist/relay/index.js"
		}
	},
	"scripts": {
		"build": "pnpm clean && tsup --config tsup.ts",
		"clean": "rm -rf dist *.tsbuildinfo",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"format": "pnpm exec biome check --write ."
	},
	"dependencies": {
		"@modelcontextprotocol/sdk": "1.26.0",
		"ioredis": "5.9.3",
		"zod": "4.3.6"
	},
	"peerDependencies": {
		"react": ">=19.0.0",
		"react-dom": ">=19.0.0"
	},
	"devDependencies": {
		"@types/node": "25.3.0",
		"@types/react": "19.2.14",
		"@types/react-dom": "19.2.3",
		"tsup": "8.5.1",
		"typescript": "5.9.3"
	}
}
