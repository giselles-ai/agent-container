{
	"name": "@giselles-ai/giselle-provider",
	"version": "0.1.0",
	"type": "module",
	"sideEffects": false,
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/giselles-ai/agent-container.git",
		"directory": "packages/giselle-provider"
	},
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		}
	},
	"scripts": {
		"build": "pnpm clean && tsup --config tsup.ts",
		"clean": "rm -rf dist *.tsbuildinfo",
		"test": "vitest run src/__tests__/*.test.ts",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"format": "pnpm exec biome check --write ."
	},
	"dependencies": {
		"@ai-sdk/provider": "3.0.7",
		"@ai-sdk/provider-utils": "4.0.13",
		"@giselles-ai/sandbox-agent": "workspace:*",
		"ioredis": "5.9.3",
		"zod": "4.3.6"
	},
	"devDependencies": {
		"@types/node": "25.3.0",
		"tsup": "8.5.1",
		"typescript": "5.9.3"
	}
}
