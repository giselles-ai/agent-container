{
  "name": "@giselles/rpa-planner",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./runtime": {
      "types": "./src/index.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm --out-dir dist --clean",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "format": "pnpm exec biome check --write ."
  },
  "dependencies": {
    "@giselles/rpa-sdk": "workspace:*",
    "ai": "6.0.59",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "tsup": "8.5.1",
    "typescript": "5.9.3"
  }
}
